

@{
    ViewBag.Title = "Contacts";
}
<link href="https://cdn.datatables.net/1.10.19/css/jquery.dataTables.min.css" rel="stylesheet" />
<script src="https://cdn.datatables.net/1.10.19/js/jquery.dataTables.min.js"></script>

<br />
<h2 class="text-center">@ViewBag.Title</h2>
<br />

<br />

<div class="container">
    <table class="table table-responsive table-striped table-hover table-md">
        <thead>
            <tr>
                <td>Action</td>
                <td>Employee Name</td>
                <td>Job</td>
                <td>Country</td>
                <td>Retired</td>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>


                        @Html.ActionLink("Edit", "EditEmp", new { id = item.EmployeeId }) |

                        @Html.ActionLink("Details", "Details", new { id = item.EmployeeId })


                    </td>
                    <td>
                        @item.Name
                    </td>
                    <td>
                        @item.Description
                    </td>
                    <td>
                        @item.Country
                    </td>
                    <td>
                        @item.IsEmployeeRetired
                    </td>
                </tr>
            }
        </tbody>

    </table>

</div>
<br /><br />

